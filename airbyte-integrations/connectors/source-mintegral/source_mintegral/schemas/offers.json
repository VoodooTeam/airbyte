{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "campaign_id": {"type": "integer"},
    "campaign_name": {"type": "string"},
    "offer_id": {"type": "integer"},
    "uuid": {"type": "string"},
    "offer_name": {"type": "string"},
    "promote_timezone": {"type": "integer"},
    "start_time": {"type": "integer"},
    "end_time": {"type": "integer"},
    "status": {"type": "string"},
    "target_geo": {"type": "string"},
    "bid_type": {"type": "string"},
    "billing_type": {"type": "string"},
    "bid_goal": {"type": "string"},
    "offer_stop_reason": {"type": "string"},
    "settlement_event": {"type": "string"},
    "tracking_method": {"type": "string"},
    "click_url": {"type": "string", "format": "uri"},
    "impression_url": {"type": "string", "format": "uri"},
    "deep_link": {"type": "string"},
    "universal_link": {"type": "string"},
    "os_version_min": {"type": "string"},
    "os_version_max": {"type": "string"},
    "custom_ad_schedule": {"type": "string"},
    "custom_ad_schedule_timezone": {"type": "integer"},
    "network": {"type": "string"},
    "target_device": {"type": "string"},
    "click_url_status": {"type": "string"},
    "budget": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "country_code": {"type": "string"},
          "total_budget": {"type": "string"},
          "total_budget_effective_time": {"type": "string"},
          "total_budget_effective_timezone": {"type": "integer"},
          "daily_cap_type": {"type": "string"},
          "daily_cap": {"type": "integer"}
        },
        "required": ["country_code", "total_budget", "total_budget_effective_time", "daily_cap_type", "daily_cap"]
      }
    },
    "user_activation": {"type": "string"},
    "maintain_by": {"type": "string"},
    "device_id_needs": {"type": "string"},
    "currency": {"type": "string"},
    "create_time": {"type": "string"},
    "bid_rate": {"type": "number"},
    "target_ad_type": {"type": "string"},
    "audience": {"type": "array"},
    "audience_exclude": {"type": "array"},
    "target_user_gender": {"type": "string"},
    "target_user_age": {"type": "string"},
    "target_custom_device": {"type": "string"},
    "target_custom_device_logic": {"type": "string"},
    "target_custom_device_brand": {"type": "string"},
    "bid_rate_by_location": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "bid_rate": {"type": "number"},
          "country_code": {"type": "string"}
        },
        "required": ["bid_rate", "country_code"]
      }
    },
    "target_custom_device_switch": {"type": "boolean"},
    "creatives": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "apply_in_area": {"type": "string"},
          "creative_id": {"type": "integer"},
          "review_time": {"type": "integer"},
          "review_status": {"type": "string"},
          "creative_name": {"type": "string"},
          "dimension": {"type": "string"},
          "file_md5": {"type": "string"},
          "creative_url": {"type": "string", "format": "uri"},
          "preview_url": {"type": "string"},
          "language": {"type": ["string", "null"]},
          "platform": {"type": ["string", "null"]},
          "creative_set_id": {"type": "integer"},
          "creative_set_name": {"type": "string"},
          "reject_reason": {"type": "string"},
          "apply_time": {"type": "integer"},
          "creative_type": {"type": "string"}
        },
        "required": [
          "apply_in_area",
          "creative_id",
          "review_time",
          "review_status",
          "creative_name",
          "dimension",
          "file_md5",
          "creative_url",
          "creative_set_id",
          "creative_set_name",
          "apply_time",
          "creative_type"
        ]
      }
    }
  },
  "required": [
    "campaign_id",
    "campaign_name",
    "offer_id",
    "uuid",
    "offer_name",
    "promote_timezone",
    "start_time",
    "end_time",
    "status",
    "target_geo",
    "bid_type",
    "billing_type",
    "bid_goal",
    "tracking_method",
    "click_url",
    "impression_url",
    "os_version_min",
    "os_version_max",
    "custom_ad_schedule_timezone",
    "network",
    "target_device",
    "click_url_status",
    "budget",
    "user_activation",
    "maintain_by",
    "device_id_needs",
    "currency",
    "create_time",
    "bid_rate",
    "target_ad_type",
    "audience",
    "audience_exclude",
    "target_user_gender",
    "target_user_age",
    "target_custom_device_logic",
    "bid_rate_by_location",
    "target_custom_device_switch",
    "creatives"
  ]
}
